# Fastlane Environment Variables Template
# Copy this to .env and fill in your values
# NEVER commit .env to git!

# Apple Developer Account
# FASTLANE_USER=your@email.com
# FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=xxxx-xxxx-xxxx-xxxx

# App Store Connect API Key (recommended over password auth)
# Generate at: https://appstoreconnect.apple.com/access/api
APP_STORE_CONNECT_API_KEY_ID=
APP_STORE_CONNECT_API_ISSUER_ID=
APP_STORE_CONNECT_API_KEY_PATH=./fastlane/AuthKey.p8

# Team IDs
# TEAM_ID=YOUR_DEVELOPER_TEAM_ID
# ITC_TEAM_ID=YOUR_APP_STORE_CONNECT_TEAM_ID

# Match (Certificate Storage)
MATCH_GIT_URL=https://github.com/brycehammond/certificates.git
MATCH_PASSWORD=  # Encryption password for certificates

# Optional: Slack notifications
# SLACK_URL=https://hooks.slack.com/services/xxx/xxx/xxx
